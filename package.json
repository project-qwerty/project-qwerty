{
  "name": "project-qwerty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run lint && vue-cli-service build",
    "lint": "npx eslint --max-warnings=0 src/*.js src/**/*.js src/*.vue src/**/*.vue",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@lottiefiles/vue-lottie-player": "^1.1.0",
    "@sentry/vue": "^8.34.0",
    "@vue/cli": "^5.0.3",
    "lint-staged": "^15.2.2",
    "vue-router": "^3.5.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.3",
    "vue": "^3.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "overrides": {
    "node-ipc@>9.2.1 <10": "9.2.1",
    "node-ipc@>10.1.0": "10.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "lint-staged": {
    "src/*.js": "npx eslint --max-warnings=0",
    "src/**/*.js": "npx eslint --max-warnings=0",
    "src/*.vue": "npx eslint --max-warnings=0",
    "src/**/*.vue": "npx eslint --max-warnings=0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
